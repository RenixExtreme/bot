'long<-0.5;
$ long;
define diagonal('width,'long) ->

		V long;
		$ long;
		> width;
		$ width;
	$ "diagonal terminada";
end;

define imprimir('dato, 'veces)->
	while(veces>0)->
		$ dato;
		
		veces<-veces-1;
	end;
end;

